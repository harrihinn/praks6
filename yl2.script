#!/bin/bash
#küsib reisijate arvu ja kohtade arvu bussis
#väljastab reisijate veoks vajaliku busside arvu
from math import ceil;
echo -n "Sisesta reisijate arv: "
read a
echo -n "Sisesta kohtade arv bussis: "
read b
c=$(($a/$b))
d=$(($a%$b))
if [ $d > 0 ]
then
echo "a=$a b=$b c=$c d=$d"
else
echo "a=$a b=$b c=$c d=$d"
fi
