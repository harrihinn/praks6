#!/bin/bash
#k체sib reisijate arvu ja kohtade arvu bussis
#v채ljastab reisijate veoks vajaliku busside arvu
from math import ceil;
echo -n "Sisesta reisijate arv: "
read a
echo -n "Sisesta kohtade arv bussis: "
read b
busse=$(($a/$b))
jaak=$(($a%$b))
if [ $jaak -gt 0 ]
then
busse=$(($busse+1))
fi
if [ $busse == 1 ]
then
i=''
else
i='i'
fi
echo "Kokku on vaja $busse buss$i j채채k=$jaak"
